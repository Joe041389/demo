@model WebAPI_Demo.Models.tCustomer
@{
    ViewBag.Title = "Edit";
}

<h2>刪除確認</h2>
<div class="panel panel-primary">
    <div class="panel-body">
        <div class="form-group">
            <label for="fname">編號</label>
            <label id="lbFid">@Model.fId</label>
           
        </div>
        <div class="form-group">
            <label for="fname">姓名</label>
            <label>@Model.fName</label>
        </div>
        <div class="form-group">
            <label for="fphone">電話</label>
            <label >@Model.fPhone</label>
        </div>
        <div class="form-group">
            <label for="femail">信箱</label>
            <label>@Model.fEmail</label>
        </div>
        <div class="form-group">
            <label for="faddress">地址</label>
            <label>@Model.fAddress</label>
        </div>
        <input type="button" value="刪除" id="btnDelete" class="btn btn-danger" />
    </div>
</div>

<script src="~/js/delete.js"></script>

