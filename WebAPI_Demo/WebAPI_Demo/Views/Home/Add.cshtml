
@{
    ViewBag.Title = "新增客戶資料";
}
<link href="~/Content/Add.css" rel="stylesheet" type="text/css" />
<h2>新增</h2>
<div class="panel panel-primary">
    <div class="panel-body">
        <form id="contact" method="post" action="">


            <div class="form-group">
                <label for="fname">姓名</label>
                <input type="text" class="form-control" id="fname" placeholder="請輸入姓名" required>
            </div>

            <div class="form-group">
                <label for="fphone">電話</label>
                <input type="text" class="form-control" id="fphone" placeholder="請輸入電話" required>
            </div>
            <div class="form-group">
                <label for="femail">信箱</label>
                <input type="email" class="form-control" id="femail" placeholder="請輸入信箱" required>
            </div>

            <div class="form-group">
                <label for="faddress">地址</label>
                <input type="text" class="form-control" id="faddress" placeholder="請輸入地址" required>
            </div>


            <input type="button" value="新增" id="btnCreate" class="btn btn-primary" />
            <div class="invalid-feedback">

            </div>

        </form>


    </div>
</div>

<script src="~/js/add.js"></script>
